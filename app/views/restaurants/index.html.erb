<h1>Hello Resto</h1>
<hr>
<ol>
  <% @restaurants.each do |restaurant| %>
    <li>
      <p><strong><%= link_to restaurant.name, restaurant_path(restaurant) %></strong> • <%= restaurant.address %> ➤ <%= restaurant.category %> </p>

    </li>
  <% end %>
</ol>
<h3><%= link_to "✚ add a new restaurant", restaurant_path("new") %></h3>
